  <script type="text/javascript">
    var $j = jQuery.noConflict();
    // Use jQuery com a variavel $j para evitar conflitos
    $j(document).ready(function () {
      $j("#forms").load("/principal").hide().fadeIn(800);
      $j("#data").mask("99/99/9999"); // onde #data é o id do campo
      $j("#cep").mask("99.999-999"); // onde #cep é o id do campo
      $j("#cpf").mask("999.999.999-99"); // onde #cpf é o id do campo
      $j("#cnpj").mask("99.999.999/9999-99"); // onde #cnpj é o id do campo
      $j("#telefone").mask("(99) 9999-9999"); // onde #telefone é o id do campo
    });
  </script>
  <script>
    function debounce(fun, mil) {
      var timer;
      return function () {
        clearTimeout(timer);
        timer = setTimeout(function () {
          fun();
        }, mil);
      };
    }
    $(function () {

      $('#adicionar').on('click', debounce(function () {
        $("#forms").empty();
        $("#forms").load("/forms").hide().fadeIn(250);
      }, 250));
    });
    $(function () {
      $("#alterar").on('click', debounce(function () {
        $("#forms").empty();
        $("#forms").load("/alterar").hide().fadeIn(250);
      }, 250));
    });
    $(function () {
      $("#adicionar_todos").on('click', debounce(function () {
        $("#forms").empty();
        $("#forms").load("/adicionar_todos").hide().fadeIn(250);
      }, 250));

    });
    $(function () {
      $("#adicionar_justificativas").on('click', debounce(function () {
        $("#forms").empty();
        $("#forms").load("/adicionar_justificativas").hide().fadeIn(250);
      }, 250));

    });
    $(function () {
      $("#home").one('click', function () {
        $("#forms").load("/principal").hide().fadeIn(800);
        $(location).attr({ href: this.href });
      });
    });
  </script>
