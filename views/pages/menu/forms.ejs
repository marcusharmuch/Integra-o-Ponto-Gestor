<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">

<head>
  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
  <script src="/views/pages/menu/vendor/jquery/jquery.js"></script>
  <script type="text/javascript" src="/views/pages/menu/vendor/jquery/jquery.maskedinput-1.1.4.pack.js" /></script>
  
</head>

<script type="text/javascript">
  $(document).ready(function () {
    $("#cpf").mask("999.999.999-99");
  });
</script>

<body>

  <!-- Breadcrumb-->
  <div class="breadcrumb-holder">
    <div class="container-fluid">
      <ul class="breadcrumb">
        <li class="breadcrumb-item">
          <a href="/login">Home</a>
        </li>
        <li class="breadcrumb-item active">Inserir </li>
      </ul>
    </div>
  </div>
  <section class="forms">
    <div class="container-fluid">
      <!-- Page Header-->
      <header>
        <h1 class="h3 display">Inserir Funcionário </h1>
      </header>
      <div class="row">
        <div class="col-lg-6">
          <div class="card">
            <div class="card-header d-flex align-items-center">
              <h4>Inserir Individual</h4>
            </div>
            <div class="card-body">

              <p>Informe o CPF</p>
              <form action="/novo" method="POST">
                <div class="form-group">
                  <input type="text" class="form-control" id="cpf" name="cpf" placeholder="somente números">
                </div>
                <div class="form-group">

                  <input type="submit" value="Inserir" class="btn btn-primary">
                </div>
              </form>

            </div>
          </div>
        </div>
      </div>
    </div>
  </section>

</body>

</html>